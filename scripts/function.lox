var a = "before a";
var b = "before a";

fun foo(a, b) {
  print "In foo";
  print "a = " + a;
  print "b = " + b;
  return;
}

a = "before a";
b = "before b";

foo(a, b);

a = "after b";
b = "after b";

foo(a, b);

fun recursive(a) {
  if (a == 0) {
    print a;
  }
  else {
    print a;
    recursive(a-1);
  }
  return;
}

recursive(10);
